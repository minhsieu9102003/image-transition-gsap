<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <%- include('./navigation') %>
        <div class="admin__side-nav">
            <h1>welcome admin</h1>
            <ul class="admin__link-list">

                <a href="/rooms">Rooms</a>
                <a href="/employees">Employees</a>
                <a href="/equipments">Equipments</a>
                <a href="/members">Members</a>
                <a href="/memberships">Memberships </a>

            </ul>
        </div>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js"
            integrity="sha512-7eHRwcbYkK4d9g/6tD/mhkf++eoTHwpNM9woBxtPUBWm67zeAfFC+HrdoE2GanKeocly/VxeLvIqwvCdk7qScg=="
            crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script>
            const tl = gsap.timeline({ defaults: { ease: 'power1.out' } });
            const adminLink = gsap.utils.toArray('.admin__link-list a')

            tl.to('.admin__side-nav', { xPercent: 100, duration: 1, ease: 'power4.inOut' })
                .to(adminLink, { xPercent: 100, duration: 1, stagger: 0.2 }, '<')

        </script>
</body>

</html>